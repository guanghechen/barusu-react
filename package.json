{
  "name": "@barusu-react/root",
  "version": "0.0.0",
  "description": "react scaffolds, components",
  "author": {
    "name": "lemon-clown",
    "url": "https://github.com/lemon-clown/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemon-clown/barusu-react.git"
  },
  "homepage": "https://github.com/lemon-clown/barusu-react/#readme",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 12.16.0"
  },
  "scripts": {
    "pre:publish": "yarn find-inconsistent && yarn sort-imports",
    ":publish": "lerna publish",
    "bootstrap": "lerna bootstrap",
    "build": "lerna exec --concurrency 1 -- 'echo && pwd && yarn build'",
    "lint:fix": "lerna exec --concurrency 1 -- 'echo && pwd && yarn lint:fix'",
    "clean": "lerna clean --yes && yarn install",
    "rebuild": "lerna exec -- yarn build",
    "new:package": "node ./node_modules/@barusu-react/template-react-package/cli.js",
    "find-inconsistent": "find-inconsistent --log-level=verbose",
    "sort-imports": "sort-imports . --log-level=verbose"
  },
  "workspaces": [
    "packages/*",
    "scaffold/*",
    "stage/laputa"
  ],
  "devDependencies": {
    "@barusu/tool-find-inconsistent-packages": "^0.0.14",
    "@barusu/tool-sort-imports": "^0.0.14",
    "@types/classnames": "^2.2.9",
    "@types/node": "^13.7.6",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/webpack": "^4.41.6",
    "@types/webpack-dev-server": "^3.10.0",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "lerna": "^3.20.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "rimraf": "^3.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2",
    "webpack": "^4.41.6",
    "webpack-dev-server": "^3.10.3"
  },
  "@barusu/tool-sort-imports": {
    "pattern": [
      "packages/**/src/**/*.{ts,tsx}",
      "packages/**/test/**/*.{ts,tsx}"
    ],
    "moduleRanks": [
      {
        "regex": "^(react|vue|angular)(?:[\/\\-][\\w\\-.\/]*)?$",
        "rank": 1.1
      },
      {
        "regex": "^mocha|chai(?:[\/][\\w\\-.\/]*)?$",
        "rank": 1.2
      },
      {
        "regex": "^[a-zA-Z\\d][\\w\\-.]*",
        "rank": 1.3
      },
      {
        "regex": "^@[a-zA-Z\\d][\\w\\-.]*\\/[a-zA-Z\\d][\\w\\-.]*",
        "rank": 1.4
      },
      {
        "regex": "^@\\/",
        "rank": 2.1
      },
      {
        "regex": "^(?:\\/|[a-zA-Z]:)",
        "rank": 3.1
      },
      {
        "regex": "^[.]{2}[\\/\\\\][^\\n]*",
        "rank": 3.2
      },
      {
        "regex": "^[.][\\/\\\\][^\\n]*",
        "rank": 3.3
      }
    ],
    "indent": "  ",
    "quote": "'",
    "semicolon": false,
    "maxColumn": 80
  }
}
