{
  "name": "@barusu-react/root",
  "version": "0.0.0",
  "description": "react scaffolds, components",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/barusu-react.git"
  },
  "homepage": "https://github.com/guanghechen/barusu-react/#readme",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 14.15.0"
  },
  "scripts": {
    ":publish": "yarn format && yarn test && lerna publish --no-private",
    "test": "lerna run --parallel test",
    "build": "lerna run --concurrency 1 build",
    "clean": "lerna clean --yes && yarn install",
    "rebuild": "yarn clean && yarn build",
    "format": "yarn find-inconsistent && yarn sort-imports && yarn lint:fix",
    "lint:fix": "lerna run --parallel lint:fix",
    "find-inconsistent": "barusu-find-inconsistent .",
    "sort-imports": "barusu-sort-imports .",
    "new:package": "node ./node_modules/@barusu-react/template-react-package/cli.js"
  },
  "workspaces": [
    "packages/*",
    "scaffold/*",
    "stage/laputa"
  ],
  "resolutions": {
    "@babel/core": "7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@teamsupercell/typings-for-css-modules-loader": "2.3.0",
    "babel-loader": "8.1.0",
    "css-loader": "4.3.0",
    "mini-css-extract-plugin": "0.11.2",
    "postcss": "7.0.34",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "4.0.1",
    "postcss-modules": "3.2.2",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "styled-components": "5.2.1",
    "stylus": "0.54.8",
    "stylus-loader": "3.0.2"
  },
  "devDependencies": {
    "@barusu/tool-find-inconsistent": "^0.0.39",
    "@barusu/tool-sort-imports": "^0.0.39",
    "@types/classnames": "^2.2.9",
    "@types/node": "^13.7.6",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/webpack": "^4.41.22",
    "@types/webpack-dev-server": "^3.11.0",
    "classnames": "^2.2.6",
    "cross-env": "~7.0.2",
    "lerna": "^3.22.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "rimraf": "^3.0.2",
    "stylus": "^0.54.8",
    "ts-node": "~9.0.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "~4.0.2",
    "webpack": "4.44.2",
    "webpack-dev-server": "3.11.0"
  },
  "@barusu/tool-sort-imports": {
    "logLevel": "verbose",
    "pattern": [
      "{packages,scaffold,stage}/*/{src,test,__test__}/**/*.{ts,tsx}",
      "!**/{test,__test__}/cases",
      "!**/node_modules/"
    ],
    "moduleRanks": [
      {
        "regex": "^(react|vue|angular)(?:[\/\\-][\\w\\-.\/]*)?$",
        "rank": 1.1
      },
      {
        "regex": "^mocha|chai(?:[\/][\\w\\-.\/]*)?$",
        "rank": 1.2
      },
      {
        "regex": "^[a-zA-Z\\d][\\w\\-.]*",
        "rank": 1.3
      },
      {
        "regex": "^@[a-zA-Z\\d][\\w\\-.]*\\/[a-zA-Z\\d][\\w\\-.]*",
        "rank": 1.4
      },
      {
        "regex": "^@\\/[^\\n]*\\.(css|styl)$",
        "rank": 2.2
      },
      {
        "regex": "^@\\/",
        "rank": 2.1
      },
      {
        "regex": "^[.][\\/\\\\][^\\n]*\\.(css|styl)$",
        "rank": 3.4
      },
      {
        "regex": "^(?:\\/|[a-zA-Z]:)",
        "rank": 3.1
      },
      {
        "regex": "^[.]{2}[\\/\\\\][^\\n]*",
        "rank": 3.2
      },
      {
        "regex": "^[.][\\/\\\\][^\\n]*",
        "rank": 3.3
      }
    ],
    "indent": "  ",
    "quote": "'",
    "semicolon": false,
    "maxColumn": 80
  }
}
