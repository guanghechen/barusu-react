{
  "name": "@barusu-react/root",
  "version": "0.0.0",
  "description": "react scaffolds, components",
  "author": {
    "name": "lemon-clown",
    "url": "https://github.com/lemon-clown/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemon-clown/barusu-react.git"
  },
  "homepage": "https://github.com/lemon-clown/barusu-react/#readme",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 12.16.0"
  },
  "scripts": {
    ":publish": "yarn format && lerna publish --no-private",
    "test": "lerna exec --concurrency 1 -- 'echo && pwd && yarn test'",
    "build": "lerna exec --concurrency 1 -- 'echo && pwd && yarn build'",
    "clean": "lerna clean --yes && yarn install",
    "rebuild": "yarn clean && yarn build",
    "format": "yarn find-inconsistent && yarn sort-imports && yarn lint:fix",
    "lint:fix": "lerna exec --concurrency 1 -- 'echo && pwd && yarn lint:fix'",
    "find-inconsistent": "barusu-find-inconsistent .",
    "sort-imports": "barusu-sort-imports .",
    "new:package": "node ./node_modules/@barusu-react/template-react-package/cli.js"
  },
  "workspaces": [
    "packages/*",
    "scaffold/*",
    "stage/laputa"
  ],
  "resolutions": {
    "postcss-modules": "3.2.2"
  },
  "devDependencies": {
    "@barusu/tool-find-inconsistent": "^0.0.37",
    "@barusu/tool-sort-imports": "^0.0.37",
    "@types/classnames": "^2.2.9",
    "@types/node": "^13.7.6",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/webpack": "^4.41.6",
    "@types/webpack-dev-server": "^3.10.0",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "lerna": "^3.22.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "rimraf": "^3.0.2",
    "ts-node": "~9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "~4.0.2",
    "webpack": "^4.41.6",
    "webpack-dev-server": "^3.10.3"
  },
  "@barusu/tool-sort-imports": {
    "logLevel": "verbose",
    "pattern": [
      "{packages,scaffold,stage}/*/{src,test,__test__}/**/*.{ts,tsx}",
      "!**/{test,__test__}/cases",
      "!**/node_modules/"
    ],
    "moduleRanks": [
      {
        "regex": "^(react|vue|angular)(?:[\/\\-][\\w\\-.\/]*)?$",
        "rank": 1.1
      },
      {
        "regex": "^mocha|chai(?:[\/][\\w\\-.\/]*)?$",
        "rank": 1.2
      },
      {
        "regex": "^[a-zA-Z\\d][\\w\\-.]*",
        "rank": 1.3
      },
      {
        "regex": "^@[a-zA-Z\\d][\\w\\-.]*\\/[a-zA-Z\\d][\\w\\-.]*",
        "rank": 1.4
      },
      {
        "regex": "^@\\/[^\\n]*\\.(css|styl)$",
        "rank": 2.2
      },
      {
        "regex": "^@\\/",
        "rank": 2.1
      },
      {
        "regex": "^[.][\\/\\\\][^\\n]*\\.(css|styl)$",
        "rank": 3.4
      },
      {
        "regex": "^(?:\\/|[a-zA-Z]:)",
        "rank": 3.1
      },
      {
        "regex": "^[.]{2}[\\/\\\\][^\\n]*",
        "rank": 3.2
      },
      {
        "regex": "^[.][\\/\\\\][^\\n]*",
        "rank": 3.3
      }
    ],
    "indent": "  ",
    "quote": "'",
    "semicolon": false,
    "maxColumn": 80
  }
}
