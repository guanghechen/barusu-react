{
  "name": "@barusu-react/webpack-config",
  "version": "0.0.1",
  "description": "webpack configuration for react+ts+stylus",
  "author": {
    "name": "lemon-clown",
    "url": "https://github.com/lemon-clown/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemon-clown/barusu-react.git"
  },
  "homepage": "https://github.com/lemon-clown/barusu-react/tree/master/scaffold/webpack-config#readme",
  "keywords": [
    "webpack",
    "react",
    "stylus",
    "typescript"
  ],
  "main": "lib/index.js",
  "type": "lib/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 12.16.0"
  },
  "files": [
    "lib/",
    ".eslintrc.js",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "prebuild": "yarn lint:fix && rimraf *.tsbuildinfo lib/",
    "build": "tsc -b tsconfig.json",
    "postbuild": "rimraf lib/package.json lib/test lib/**/*.tsbuildinfo && mv lib/src/* lib && rimraf lib/src/",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@barusu-react/webpack-rule-css": "^0.0.1",
    "@barusu-react/webpack-rule-tsx": "^0.0.1",
    "@barusu-react/webpack-util": "^0.0.1",
    "@barusu/option-util": "^0.0.1",
    "@teamsupercell/typings-for-css-modules-loader": "^2.1.0",
    "@types/case-sensitive-paths-webpack-plugin": "^2.1.4",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/webpack": "^4.41.6",
    "@types/webpack-dev-server": "^3.10.0",
    "@types/webpack-merge": "^4.1.5",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "copy-webpack-plugin": "^5.1.1",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-safe-parser": "^4.0.2",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "resolve": "^1.15.1",
    "resolve-url-loader": "^3.1.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^2.3.5",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.6",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@barusu/eslint-config": "^0.0.1"
  }
}
