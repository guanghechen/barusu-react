{
  "name": "@barusu-react/rollup-config",
  "version": "0.0.7",
  "description": "rollup for react+ts+stylus",
  "author": {
    "name": "lemon-clown",
    "url": "https://github.com/lemon-clown/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemon-clown/barusu-react.git"
  },
  "homepage": "https://github.com/lemon-clown/barusu-react/tree/master/scaffold/rollup-config#readme",
  "keywords": [
    "rollup",
    "typescript",
    "stylus"
  ],
  "main": "lib/cjs/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 12.16.0"
  },
  "files": [
    "lib/",
    ".eslintrc.js",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map"
  ],
  "scripts": {
    "start": "rollup -w -c rollup.config.js",
    "prebuild": "rimraf lib/",
    "build": "rollup -c rollup.config.js",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "prepublishOnly": "cross-env ROLLUP_USE_SOURCE_MAP=false yarn build"
  },
  "dependencies": {
    "@barusu-react/rollup-plugin-postcss-dts": "^0.0.7",
    "@barusu/option-util": "^0.0.9",
    "@barusu/package-util": "^0.0.9",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-multi-entry": "^3.0.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@types/autoprefixer": "^9.7.0",
    "@types/eslint": "^6.1.8",
    "@types/postcss-url": "^8.0.1",
    "@types/rollup-plugin-peer-deps-external": "^2.2.0",
    "autoprefixer": "^9.7.4",
    "builtin-modules": "^3.1.0",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-url": "^8.0.0",
    "rollup": "^2.6.1",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-typescript2": "^0.27.0"
  },
  "optionalDependencies": {
    "stylus": "^0.54.7"
  },
  "devDependencies": {
    "@barusu/eslint-config": "^0.0.9",
    "@barusu/rollup-config": "^0.0.9",
    "cross-env": "^7.0.2"
  }
}
