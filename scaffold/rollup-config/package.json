{
  "name": "@barusu-react/rollup-config",
  "version": "0.0.20",
  "description": "rollup for react+ts+stylus",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/barusu-react.git"
  },
  "homepage": "https://github.com/guanghechen/barusu-react/tree/master/scaffold/rollup-config#readme",
  "keywords": [
    "rollup",
    "typescript",
    "stylus"
  ],
  "main": "lib/cjs/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 14.15.0"
  },
  "files": [
    "lib/",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js",
    "prebuild": "rimraf lib/",
    "prepublishOnly": "cross-env ROLLUP_SHOULD_SOURCEMAP=false yarn build"
  },
  "dependencies": {
    "@barusu-react/rollup-plugin-postcss-dts": "^0.0.20",
    "@guanghechen/rollup-config": "1.0.6",
    "@rollup/plugin-multi-entry": "4.0.0",
    "@types/postcss-url": "^8.0.1",
    "autoprefixer": "10.2.5",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-url": "10.1.1"
  },
  "optionalDependencies": {
    "stylus": "^0.54.8"
  },
  "peerDependencies": {
    "fs-extra": "^9.1.0",
    "rollup": "^2.40.0"
  }
}
