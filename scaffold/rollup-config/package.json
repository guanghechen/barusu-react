{
  "name": "@barusu-react/rollup-config",
  "version": "0.0.20",
  "description": "rollup for react+ts+stylus",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/barusu-react.git"
  },
  "homepage": "https://github.com/guanghechen/barusu-react/tree/master/scaffold/rollup-config#readme",
  "keywords": [
    "rollup",
    "typescript",
    "stylus"
  ],
  "main": "lib/cjs/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 14.15.0"
  },
  "files": [
    "lib/",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map"
  ],
  "scripts": {
    "start": "rollup -w -c rollup.config.js",
    "prebuild": "rimraf lib/",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "prepublishOnly": "cross-env ROLLUP_USE_SOURCE_MAP=false yarn build",
    "test": "echo 'no test yet'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix"
  },
  "dependencies": {
    "@barusu-react/rollup-plugin-postcss-dts": "^0.0.20",
    "@barusu/util-cli": "^0.0.43",
    "@barusu/util-option": "^0.0.43",
    "@rollup/plugin-commonjs": "17.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-multi-entry": "4.0.0",
    "@rollup/plugin-node-resolve": "11.0.1",
    "@types/autoprefixer": "^9.7.2",
    "@types/postcss-url": "^8.0.1",
    "@types/rollup-plugin-peer-deps-external": "2.2.0",
    "autoprefixer": "^9.8.6",
    "builtin-modules": "3.2.0",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-url": "^9.0.0",
    "rollup": "2.35.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-typescript2": "0.28.0"
  },
  "optionalDependencies": {
    "stylus": "^0.54.8"
  },
  "devDependencies": {
    "@barusu/eslint-config": "^0.0.43",
    "@barusu/rollup-config": "^0.0.43"
  }
}
