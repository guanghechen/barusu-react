{
  "name": "@barusu-react/rollup-plugin-postcss-dts",
  "version": "0.0.16",
  "description": "generate *.d.ts for *.styl|*.scss|*.sass|*.css files",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/barusu-react.git"
  },
  "homepage": "https://github.com/guanghechen/barusu-react/tree/master/scaffold/rollup-plugin-postcss-dts#readme",
  "keywords": [
    "rollup-plugin",
    "postcss",
    "css d.ts"
  ],
  "main": "lib/cjs/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 14.15.0"
  },
  "files": [
    "lib/",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map"
  ],
  "scripts": {
    "start": "rollup -w -c rollup.config.js",
    "prebuild": "rimraf lib/",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "prepublishOnly": "cross-env ROLLUP_USE_SOURCE_MAP=false yarn build",
    "test": "echo 'no test yet'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix"
  },
  "resolutions": {
    "postcss-modules": "3.2.2"
  },
  "dependencies": {
    "@barusu/util-option": "^0.0.41",
    "@types/fs-extra": "^9.0.1",
    "@types/reserved-words": "^0.1.0",
    "fs-extra": "^9.0.1",
    "postcss-modules": "3.2.2",
    "reserved-words": "^0.1.2",
    "rollup-plugin-postcss": "^3.1.8"
  },
  "devDependencies": {
    "@barusu/eslint-config": "^0.0.41",
    "@barusu/rollup-config": "^0.0.41",
    "@rollup/pluginutils": "^4.0.0",
    "rollup": "2.28.1"
  }
}
