{
  "name": "@barusu-react/webpack-rule-css",
  "version": "0.0.5",
  "description": "css|stylus|sass rule of <webpack config>.module.rules",
  "author": {
    "name": "lemon-clown",
    "url": "https://github.com/lemon-clown/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemon-clown/barusu-react.git"
  },
  "homepage": "https://github.com/lemon-clown/barusu-react/tree/master/scaffold/webpack-rule-css#readme",
  "main": "lib/cjs/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 12.16.0"
  },
  "files": [
    "lib/",
    ".eslintrc.js",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map"
  ],
  "scripts": {
    "start": "rollup -w -c rollup.config.js",
    "prebuild": "rimraf lib/",
    "build": "rollup -c rollup.config.js",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "prepublishOnly": "cross-env ROLLUP_USE_SOURCE_MAP=false yarn build"
  },
  "dependencies": {
    "css-loader": "^3.4.2",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "style-loader": "^1.1.3"
  },
  "peerDependencies": {
    "@types/webpack": "^4.41.6",
    "webpack": "^4.41.6"
  },
  "optionalDependencies": {
    "@teamsupercell/typings-for-css-modules-loader": "^2.1.0",
    "node-sass": "^4.13.1",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2"
  },
  "devDependencies": {
    "@barusu/eslint-config": "^0.0.7",
    "@barusu/rollup-config": "^0.0.7",
    "@types/webpack": "^4.41.6",
    "cross-env": "^7.0.2",
    "rollup": "^1.31.1",
    "webpack": "^4.41.6"
  }
}
